{
  "name": "slacker",
  "version": "1.0.0",
  "description": "Github Action that allows you to send messages to Slack via a webhook.",
  "main": "build/main.js",
  "repository": "git@github.com:zrosenbauer/slacker.git",
  "author": "@zrosenbauer",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "type:check": "tsc --noEmit",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:check": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "package": "ncc build dist/main.js --source-map --license licenses.txt --out build",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@slack/webhook": "^7.0.1"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.18.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.52.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@3.6.3"
}
